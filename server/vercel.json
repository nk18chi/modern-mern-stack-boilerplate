{
    "version": 2,
    "buildCommand":"yarn build",
    "installCommand": "yarn install",
    "builds": [
      {
        "src": "build/index.js",
        "use": "@vercel/node",
        "config": { "includeFiles": ["build/**"] }
      }
    ],
    "routes": [
        {
          "src": "/(.*)",
          "dest": "build/index.js"
        }
      ]
  }